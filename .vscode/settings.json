{
  "cSpell.files": ["src/**"],
  "cSpell.words": [
    "Arrayformat",
    "asignee",
    "authorised",
    "axios",
    "Checkboxgroup",
    "composables",
    "daterange",
    "DATETIME",
    "duedate",
    "fieldvalues",
    "fusioncharts",
    "Gantt",
    "gapless",
    "Geofencing",
    "groupby",
    "headlessui",
    "Heroicons",
    "is_gapless",
    "kanban",
    "middlewares",
    "Multifile",
    "orderindex",
    "phonenumber",
    "pinia",
    "pkce",
    "popout",
    "prettysize",
    "rnel",
    "SENSEHAWK",
    "showpercentvalues",
    "Splitpanes",
    "startdate",
    "stringifier",
    "tabler",
    "taskmapper",
    "uids",
    "unarchived",
    "uppy",
    "vite",
    "vuedraggable",
    "vueform",
    "vueuse",
    "xsmall"
  ],
  "editor.tabSize": 2,
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "vue.codeActions.enabled": false,
  "css.customData": [".vscode/tailwind.json"],
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "search.exclude": {
    "analysis.html": true,
    "pnpm-lock.yaml": true,
    "**/.git": true,
    "**/node_modules": true,
    "**/trash": true
  },
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
      {
          "text": "NOTE:",
          "color": "white",
          "backgroundColor": "dodgerBlue",
      }
  ],
  "todohighlight.include": [
      "**/*.js",
      "**/*.jsx",
      "**/*.ts",
      "**/*.tsx",
      "**/*.html",
      "**/*.php",
      "**/*.css",
      "**/*.scss",
      "**/*.json",
      "**/*.vue"
  ],
  "todohighlight.exclude": [
      "**/node_modules/**",
      "**/bower_components/**",
      "**/dist/**",
      "**/build/**",
      "**/.vscode/**",
      "**/.github/**",
      "**/_output/**",
      "**/*.min.*",
      "**/*.map",
      "**/.next/**"
  ],
  "todohighlight.maxFilesForSearch": 5120,
  "todohighlight.toggleURI": false,
  "sonarlint.connectedMode.project": {
    "connectionId": "sensehawk",
    "projectKey": "sensehawk_hawk-ui-web"
  }
}
